{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "compilerOptions": {
    "lib": ["esnext", "es2020", "dom", "DOM.Iterable"],
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es2018"
  },
  "include": ["src/**/*"],
  "exclude": [
    "node_modules",
    "src\\libs"
  ]
}
